{
  "description": "renders reactive conditional with undefined false branch as comment markers",
  "level": "method",
  "method": "renderConditional",
  "input": {
    "type": "conditional",
    "condition": "isChecked()",
    "conditionType": null,
    "reactive": true,
    "whenTrue": {
      "type": "element",
      "tag": "svg",
      "attrs": [],
      "events": [],
      "ref": null,
      "children": [],
      "slotId": null,
      "needsScope": false
    },
    "whenFalse": {
      "type": "expression",
      "expr": "undefined",
      "typeInfo": null,
      "reactive": false,
      "slotId": null
    },
    "slotId": "slot_1"
  },
  "assertions": [
    {
      "type": "output-not-empty"
    }
  ],
  "expected": {
    "go-template": {
      "contains": [
        "{{else}}",
        "{{bfComment \"cond-start:slot_1\"}}",
        "{{bfComment \"cond-end:slot_1\"}}"
      ]
    },
    "hono": {
      "template": "{isChecked() ? <svg bf-c=\"slot_1\" /> : <>{bfComment(\"cond-start:slot_1\")}{null}{bfComment(\"cond-end:slot_1\")}</>}"
    }
  }
}
