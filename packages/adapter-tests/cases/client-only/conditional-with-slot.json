{
  "description": "renders client-only conditional with slotId as comment markers",
  "level": "method",
  "method": "renderConditional",
  "input": {
    "type": "conditional",
    "condition": "todos().every(t => t.done)",
    "conditionType": null,
    "reactive": false,
    "whenTrue": {
      "type": "element",
      "tag": "span",
      "attrs": [],
      "events": [],
      "ref": null,
      "children": [
        {
          "type": "text",
          "value": "All done!"
        }
      ],
      "slotId": null,
      "needsScope": false
    },
    "whenFalse": {
