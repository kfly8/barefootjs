      "expr": "null",
      "typeInfo": null,
      "reactive": false,
      "slotId": null
    },
    "slotId": "slot_3",
    "clientOnly": true
  },
  "assertions": [
    {
      "type": "contains-text",
      "text": "slot_3"
    },
    {
      "type": "output-not-empty"
    }
  ]
